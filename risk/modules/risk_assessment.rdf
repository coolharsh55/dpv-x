  <rdf:Description rdf:about="https://w3id.org/dpv/risk#RiskAssessmentTechnique">
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#RiskManagementProcedure"/>
    <skos:prefLabel xml:lang="en">Risk Assessment Technique</skos:prefLabel>
    <skos:definition xml:lang="en">A technique or method used to analyse and identify risk levels, sources, likelihoods, severities, and other necessary information required to conduct risk management procedures</skos:definition>
    <dct:source rdf:resource="https://www.iso.org/standard/72140.html"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
