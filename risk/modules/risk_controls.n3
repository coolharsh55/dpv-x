    rdfs:isDefinedBy risk: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:RiskMitigationMeasure ;
    skos:definition "Risk Mitigation Measure that uses controls to monitor events"@en ;
    skos:narrower risk:MonitorConsequence,
        risk:MonitorImpact,
        risk:MonitorRisk,
        risk:MonitorRiskControl,
        risk:MonitorRiskSource,
        risk:MonitorVulnerabilities ;
    skos:note "Monitoring can be associated with characteristics such as assessing or detecting whether something is active, operational, performant, effective, has potential to materialise, is materialising, or has already materialised."@en ;
    skos:prefLabel "Control Monitors"@en ;
    dpv:isSubTypeOf dpv:RiskMitigationMeasure .

