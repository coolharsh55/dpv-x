risk:Risk_ControlsConcepts a skos:Collection ;
    skos:member risk:AvoidSource,
        risk:ChangeConsequence,
        risk:ChangeImpact,
        risk:ControlConsequence,
        risk:ControlMonitors,
        risk:ControlRiskSource,
        risk:HaltSource,
        risk:MonitorConsequence,
        risk:MonitorImpact,
        risk:MonitorRisk,
        risk:MonitorRiskControl,
        risk:MonitorRiskSource,
        risk:MonitorVulnerabilities,
        risk:ReduceLikelihood,
        risk:ReduceSeverity,
        risk:RemoveConsequence,
        risk:RemoveImpact,
        risk:RemoveSource,
        risk:ShareRisk ;
    skos:prefLabel "Risk_Controls Concepts"^^xsd:string .

