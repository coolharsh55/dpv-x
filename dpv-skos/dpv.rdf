  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#AnonymisedData">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#NonPersonalData"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <skos:definition xml:lang="en">Personal Data that has been (fully and completely) anonymised so that it is no longer considered Personal Data</skos:definition>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#NonPersonalData"/>
