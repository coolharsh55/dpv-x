    rdfs:isDefinedBy dpvs: ;
    sw:term_status "accepted"@en ;
    skos:definition "The duration or temporal limitation"@en ;
    skos:prefLabel "Duration"@en .

dpvs:Frequency a rdfs:Class,
        skos:Concept ;
    dct:created "2022-02-16"^^xsd:date ;
    rdfs:isDefinedBy dpvs: ;
    sw:term_status "accepted"@en ;
