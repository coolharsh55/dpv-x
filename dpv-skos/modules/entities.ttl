    rdfs:subClassOf dpvs:Authority ;
    sw:term_status "accepted"@en ;
    skos:broaderTransitive dpvs:Authority ;
    skos:definition "e.g. EU, EDPB"@en ;
    skos:prefLabel "SupraNationalAuthority"@en .

dpvs:ThirdParty a rdfs:Class,
        skos:Concept ;
    dct:created "2019-06-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:source <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_10/oj> ;
