    sw:term_status "accepted"@en ;
    skos:broaderTransitive dpvs:LegalEntity ;
    skos:definition "A representative of a legal entity"@en ;
    skos:prefLabel "Representative"@en .

dpvs:SupraNationalAuthority a rdfs:Class,
