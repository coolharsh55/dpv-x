    skos:prefLabel "Legal Entity"@en .

dpvs:NationalAuthority a rdfs:Class,
        skos:Concept ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:source <http://purl.org/adms> ;
    rdfs:isDefinedBy dpvs: ;
    rdfs:subClassOf dpvs:Authority ;
    sw:term_status "accepted"@en ;
    skos:broaderTransitive dpvs:Authority ;
    skos:definition "e.g. DPA, but not all \"DPA\" are national authority e.g. EDPS and EDPB"@en ;
    skos:prefLabel "NationalAuthority"@en .

dpvs:NaturalPerson a rdfs:Class,
        skos:Concept ;
    dct:created "2022-02-09"^^xsd:date ;
