        dpvs:EnterIntoContract,
        dpvs:LegalBasis,
        dpvs:LegalObligation,
        dpvs:LegitimateInterest,
        dpvs:LegitimateInterestOfController,
        dpvs:LegitimateInterestOfDataSubject,
        dpvs:LegitimateInterestOfThirdParty,
        dpvs:OfficialAuthorityOfController,
