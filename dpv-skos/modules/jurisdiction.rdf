    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/dpv-skos#hasLocation"/>
    <skos:prefLabel xml:lang="en">has country</skos:prefLabel>
    <skos:definition xml:lang="en">Indicates applicability of specified country</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
