    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#LocalLocation"/>
    <skos:prefLabel xml:lang="en">Within Device</skos:prefLabel>
    <skos:definition xml:lang="en">Location is local and entirely within a device, such as a smartphone</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-06-15</dct:created>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-10-05</dct:modified>
    <sw:term_status xml:lang="en">modified</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#hasThirdCountry">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
