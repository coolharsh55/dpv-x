    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#hasCountry">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
