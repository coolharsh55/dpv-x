        dpvs:LocalLocation,
        dpvs:Location,
        dpvs:LocationFixture,
        dpvs:LocationLocality,
        dpvs:PrivateLocation,
        dpvs:PublicLocation,
        dpvs:RandomLocation,
        dpvs:Region,
