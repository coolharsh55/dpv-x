  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#DataProtectionAuthority">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#Authority"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#Authority"/>
    <skos:prefLabel xml:lang="en">Data Protection Authority</skos:prefLabel>
    <skos:definition xml:lang="en">An authority tasked with overseeing legal compliance regarding privacy and data protection laws.</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
