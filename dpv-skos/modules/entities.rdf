    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#Representative">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <skos:prefLabel xml:lang="en">Representative</skos:prefLabel>
    <skos:definition xml:lang="en">A representative of a legal entity</skos:definition>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_27/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
