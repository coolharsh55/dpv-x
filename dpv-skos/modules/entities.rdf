    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos#DataExporter">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <skos:prefLabel xml:lang="en">Data Exporter</skos:prefLabel>
    <skos:definition xml:lang="en">An entity that 'exports' data where exporting is considered a form of data transfer</skos:definition>
    <skos:note xml:lang="en">The EU, in particular the EDPB, uses data exporter the context of cross-border data transfers/flows. These concepts are not bound by jurisdictional or geopolitical scopes within DPV and can thus be used for any notion of exporting</skos:note>
    <dct:source rdf:resource="https://edpb.europa.eu/our-work-tools/our-documents/recommendations/recommendations-012020-measures-supplement-transfer_en"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos#"/>
  </rdf:Description>
