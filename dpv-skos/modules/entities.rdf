    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <skos:broaderTransitive rdf:resource="https://w3id.org/dpv/dpv-skos#GovernmentalOrganisation"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#LegalEntity"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#GovernmentalOrganisation"/>
    <skos:prefLabel xml:lang="en">Authority</skos:prefLabel>
    <skos:definition xml:lang="en">An authority with the power to create or enforce laws, or determine their compliance.</skos:definition>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
