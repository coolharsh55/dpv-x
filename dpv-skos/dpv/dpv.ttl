dpv:AutomatedDecisionMaking a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that involves automated decision making"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Automated Decision Making"@en .

dpv:Consent a skos:Concept ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:description "Consent of the data subject regarding processing of their personal data"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_11/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Consent"@en .

dpv:DataSource a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "The source or origin of data"@en ;
    rdfs:comment "Source is direct point of data collection; 'origin' would indicate the original/others points of where the data originates from"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Data Source"@en .

dpv:EvaluationScoring a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that involves evaluation and scoring of individuals"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Evaluation and Scoring"@en .

dpv:InnovativeUseOfNewTechnologies a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that involves use of innovative and new technologies"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Innovative Use of New Technologies"@en .

dpv:LargeScaleProcessing a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that takes place at large scales"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Large Scale Processing"@en .

dpv:LegalBasis a skos:Concept ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:description "The Legal basis used to justify processing of personal data"@en ;
    dct:modified "2020-11-04"^^xsd:date ;
    rdfs:comment "Legal basis (plural: legal bases) are defined by legislations and regulations, whose applicability is usually restricted to specific jurisdictions."@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Legal Basis"@en .

dpv:MatchingCombining a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that involves matching and combining of personal data"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Matching and Combining"@en .

dpv:PersonalDataHandling a skos:Concept ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Axel Polleres"^^xsd:string,
        "Javier Fernández"^^xsd:string ;
    dct:description "A high-level Class to describe 'data handling'. This can consist of personal data being processed for a purpose, involving entities, using technical and organisational measures, applicable risks, rights, and legal basis."@en ;
    dct:modified "2020-11-04"^^xsd:date ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Personal Data Handling"@en .

dpv:Risk a skos:Concept ;
    dct:created "2020-11-18"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A risk or possibility or uncertainty of negative effects, impacts, or consequences."@en ;
    rdfs:comment "Risks can be associated with one or more different concepts such as purpose, processing, personal data, technical or organisational measure."@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Risk"@en .

dpv:SystematicMonitoring a skos:Concept ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string,
        "Piero Bonatti"^^xsd:string ;
    dct:description "Processing that involves systematic monitoring of individuals"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "Systematic Monitoring"@en .

dpv:hasAddress a rdf:Property ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies address of a legal entity such as street address or pin code"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has address"@en .

dpv:hasAlgorithmicLogic a rdf:Property ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P. Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Indicates the logic used in processing such as for automated decision making"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has algorithmic logic"@en .

dpv:hasConsentNotice a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Specifies the notice provided in context of consent"@en ;
    rdfs:comment "The actual notice that the Data Subject received to consent to, either a text or link to a document, which should be usable to decide whether the form or consent was compliant to legislation, e.g. documenting how the user has been informed about rights and implications (such as, right to data portability,right to recitffy, right to erasure, right to restrict processing, right to object, rights regarding automated decision making or profiling, processors, third parties, sub-processors, outside-EEA transfers, automated decision-making, or other necessary details of the privacy-policy). Can be TextOrDocumentOrURI."@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has consent notice"@en .

dpv:hasConsequences a rdf:Property ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P. Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Indicates consequences of processing such as those for Data Subjects in relation to automated decision making"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has consequences"@en .

dpv:hasContact a rdf:Property ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies contact details of a legal entity such as phone  or email"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has contact"@en .

dpv:hasContext a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:description "Indicates a purpose is restricted to the specified context(s)"@en ;
    rdfs:domain dpv:Purpose ;
    rdfs:range dpv:Context ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has context"@en .

dpv:hasDataController a rdf:Property ;
    dct:created "2019-04-04"^^xsd:date ;
    dct:creator "Axel Polleres"^^xsd:string,
        "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Javier Fernández"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Indicates association with a Data Controller."@en ;
    dct:modified "2020-11-04"^^xsd:date ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has data controller"@en .

dpv:hasDataSource a rdf:Property ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg P. Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Indicates the source or origin of data being processed"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has data source"@en .

dpv:hasDataSubject a rdf:Property ;
    dct:created "2019-04-04"^^xsd:date ;
    dct:creator "Axel Polleres"^^xsd:string,
        "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Javier Fernández"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Indicates association with a specific Data Subject or a group or category of Data Subjects."@en ;
    dct:modified "2020-11-04"^^xsd:date ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has data subject"@en .

dpv:hasDuration a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Axel Polleres"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string,
        "Rob Brennan"^^xsd:string ;
    dct:description "Indicates information about duration"@en ;
    rdfs:isDefinedBy <https://www.specialprivacy.eu/> ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has duration"@en .

dpv:hasExpiry a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Generic property specifying when or under which condition(s) the consent will expire"@en ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has expiry"@en .

dpv:hasExpiryCondition a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Specifies the condition or event that determines the expiry of consent"@en ;
    rdfs:comment "Can be TextOrDocumentOrURI"@en ;
    rdfs:subPropertyOf dpv:expiry ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has expiry condition"@en .

dpv:hasExpiryTime a rdf:Property ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Bud Bruegger"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Mark Lizar"^^xsd:string ;
    dct:description "Specifies the expiry time or duration for consent"@en ;
    rdfs:range time:TemporalEntity ;
    rdfs:subPropertyOf dpv:expiry ;
    sw:term_status "accepted"@en ;
    skos:prefLabel "has expiry time"@en .

