    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eva Schlehahn</dct:creator>
    <skos:definition xml:lang="en">compliance with a legal obligation</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
