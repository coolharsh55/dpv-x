    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#RightFulfilmentNotice"/>
    <skos:definition xml:lang="en">A Notice provided in fulfilment of GDPR's Art.13 regarding information to be provided where personal data are collected from the data subject</skos:definition>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
