  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A46-2-f">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:note>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <skos:definition xml:lang="en">An approved certification mechanism pursuant to GDPR Article 42 together with binding and enforceable commitments of the controller or processor in the third country to appy the appropriate safeguards, including as regards individuals` rights</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_f/oj"/>
    <skos:prefLabel xml:lang="en">Art 46(2-f)</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A49-1-e">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:prefLabel xml:lang="en">Art 49(1-e)</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is necessary for the establishment, exercise or defence of legal claims.</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_e/oj"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A46-3-a">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:definition xml:lang="en">Contractual clauses with controller, processor or recipient of the personal data in the third country or the international organisation.</skos:definition>
    <skos:prefLabel xml:lang="en">Art 46(3-a)</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards exist. Transfer does requires specific authorisation from a Supervisor Authority.</skos:note>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_3/pnt_a/oj"/>
  </rdf:Description>
