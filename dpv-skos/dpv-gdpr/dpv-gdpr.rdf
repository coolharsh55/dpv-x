    <rdfs:comment xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</rdfs:comment>
    <skos:broader rdf:resource="http://www.w3.org/ns/dpv#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">Binding corporate rules</dct:description>
  </rdf:Description>
