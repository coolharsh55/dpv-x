    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A9-2-j">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:prefLabel xml:lang="en">Art 9(2-j) public interest, scientific research, statistical purpose</skos:prefLabel>
    <skos:definition xml:lang="en">public interest, scientific or historical research purposes or statistical purposes based on Union or Member State law</skos:definition>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#PublicInterest"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eva Schlehahn</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
