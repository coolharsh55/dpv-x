  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/gdpr#A46-2-e">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:definition xml:lang="en">An approved code of conduct pursuant to GDPR Article 40 together with binding and enforceable commitments of the controller or processor in the third country to apply the appropriate safeguards, including as regards individuals´ rights</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:note>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
