    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has not Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
