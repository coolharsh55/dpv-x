    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_6/par_1/pnt_a/oj"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <skos:definition xml:lang="en">consent of the data subject</skos:definition>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-10</dct:created>
    <skos:prefLabel xml:lang="en">Art.6(1-a) regular consent</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#Consent"/>
