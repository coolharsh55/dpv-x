    <skos:prefLabel xml:lang="en">Data Transfer Tool</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#TechnicalOrganisationalMeasure"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <skos:definition xml:lang="en">A legal instrument or tool intended to assist or justify data transfers</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
  </rdf:Description>
