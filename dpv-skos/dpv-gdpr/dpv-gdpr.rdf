    <skos:definition xml:lang="en">Right to withdraw consent at any time</skos:definition>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#DirectDataCollectionNotice">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#DataSubjectRight"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
