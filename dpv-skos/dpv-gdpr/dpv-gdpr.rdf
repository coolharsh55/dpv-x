    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <skos:definition xml:lang="en">protection of the vital interests</skos:definition>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <skos:prefLabel xml:lang="en">Art 9(2-c) protect vital interest</skos:prefLabel>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#VitalInterest"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A49-1-f">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_f/oj"/>
    <skos:prefLabel xml:lang="en">Art 49(1-f) protect vital interests</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the person is physically or legally incapable of giving consent.</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#VitalInterestOfNatualPerson"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A7-3">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataSubjectRight"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#DataSubjectRight"/>
