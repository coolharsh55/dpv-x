    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_j/oj"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#DPIAProcessingRecommendation">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/dpv-gdpr#"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
    <skos:prefLabel xml:lang="en">DPIA Processing Recommendation</skos:prefLabel>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#AuditStatus"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-skos#AuditStatus"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
