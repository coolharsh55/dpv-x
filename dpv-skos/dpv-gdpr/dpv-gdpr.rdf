  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#A49-1-a">
    <skos:broader rdf:resource="http://www.w3.org/ns/dpv#LegalBasis"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:prefLabel xml:lang="en">Art 49(1-a)</skos:prefLabel>
    <rdfs:comment xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</rdfs:comment>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj"/>
    <dct:description xml:lang="en">The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards.</dct:description>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#A19">
    <dct:description xml:lang="en">Right to be notified in case of rectification or erasure of personal data or restriction of processing</dct:description>
    <skos:prefLabel xml:lang="en">A19 Right to Rectification</skos:prefLabel>
