    <skos:prefLabel xml:lang="en">Binding Corporate Rules (BCR)</skos:prefLabel>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_20/oj"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#DataTransferTool"/>
  </rdf:Description>
