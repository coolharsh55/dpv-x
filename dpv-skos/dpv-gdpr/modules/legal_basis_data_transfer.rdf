    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A49-1-e">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <skos:prefLabel xml:lang="en">Art 49(1-e) legal claims</skos:prefLabel>
    <skos:definition xml:lang="en">The transfer is necessary for the establishment, exercise or defence of legal claims.</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_e/oj"/>
