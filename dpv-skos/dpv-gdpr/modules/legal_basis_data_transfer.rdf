    <skos:prefLabel xml:lang="en">Art 46(2-b) Binding Corporate Rules (BCR)</skos:prefLabel>
    <skos:definition xml:lang="en">Binding corporate rules</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:note>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_b/oj"/>
