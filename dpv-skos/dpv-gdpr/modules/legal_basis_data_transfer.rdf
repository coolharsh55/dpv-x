  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-skos/dpv-gdpr#A46-2-c">
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-skos#LegalBasis"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos#DataTransferLegalBasis"/>
    <skos:broader rdf:resource="https://w3id.org/dpv/dpv-skos/dpv-gdpr#SCCByCommission"/>
    <skos:prefLabel xml:lang="en">Art 46(2-c) Standard Contractual Clauses (SCC) by EC</skos:prefLabel>
    <skos:definition xml:lang="en">Standard data protection clauses adopted by the Commission</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Third country has appropriate safeguards. Transfer does not require specific authorisation from a Supervisor Authority.</skos:note>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
