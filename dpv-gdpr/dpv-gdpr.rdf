  <rdf:Description rdf:about="https://w3id.org/dpv/gdpr#A49-1-f">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <skos:prefLabel xml:lang="en">Art 49(1-f)</skos:prefLabel>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#DataTransferLegalBasis"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#VitalInterestOfNatualPerson"/>
    <skos:definition xml:lang="en">The transfer is necessary in order to protect the vital interests of the data subject or of other persons, where the person is physically or legally incapable of giving consent.</skos:definition>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_f/oj"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/gdpr#A9-2-d">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Bud Bruegger</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Eva Schlehahn</dct:creator>
    <skos:prefLabel xml:lang="en">Art 9(2-d)</skos:prefLabel>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#LegitimateInterest"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <skos:definition xml:lang="en">legitimate activities with appropriate safeguards by a foundation, association or any other not-for-profit body with a political, philosophical, religious or trade union aim and on condition that the processing relates solely to the members or to former members of the body or to persons who have regular contact with it in connection with its purposes and that the personal data are not disclosed outside that body without the consent of the data subjects;</skos:definition>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_9/par_2/pnt_d/oj"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/gdpr#A49-1-a">
    <skos:inScheme rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#Consent"/>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#DataTransferLegalBasis"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/gdpr#"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <skos:prefLabel xml:lang="en">Art 49(1-a)</skos:prefLabel>
    <skos:note xml:lang="en">Transfer from EU to a third country. Third country has no Adequacy Decision. Appropriate safeguards do not exist.</skos:note>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <skos:definition xml:lang="en">The data subject has explicitly consented to the proposed transfer, after having been informed of the possible risks of such transfers for the data subject due to the absence of an adequacy decision and appropriate safeguards.</skos:definition>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_1/pnt_a/oj"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
