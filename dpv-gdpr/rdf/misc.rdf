  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#SCCBySupervisoryAuthority">
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">Standard data protection clauses adopted by a supervisory authority and approved by the Commission pursuant to the examination procedure referred to in GDPR Article 93(2)</dct:description>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <rdfs:label xml:lang="en">SCCs adopted by Supervisory Authority</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_d/oj"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#SCCByCommission">
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <dct:description xml:lang="en">Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)</dct:description>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:label xml:lang="en">SCCs adopted by Commission</rdfs:label>
