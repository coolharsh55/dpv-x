    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <rdfs:label xml:lang="en">Standard Data Protection Clauses</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#SafeguardForDataTransfer"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv-gdpr#BindingCorporateRules">
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-22</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">David Hickey</dct:creator>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#SafeguardForDataTransfer"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#Policy"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <rdfs:label xml:lang="en">Binding Corporate Rules</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_20/oj"/>
    <dct:description xml:lang="en">Binding corporate rules (BCR) are data protection policies adhered to by companies established in the EU for transfers of personal data outside the EU within a group of undertakings or enterprises.</dct:description>
  </rdf:Description>
