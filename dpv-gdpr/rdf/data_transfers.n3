@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <http://www.w3.org/ns/dpv#> .
@prefix dpv-gdpr: <http://www.w3.org/ns/dpv-gdpr#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dpv-gdpr:AdHocContractualClauses a rdfs:Class ;
    rdfs:label "AdHoc Contractual Clauses"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Contractual Clauses not drafted by the EU Commission, e.g. by the Controller"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf> ;
    rdfs:subClassOf dpv:Contract,
        dpv-gdpr:DataTransferTool ;
    sw:term_status "accepted"@en .

dpv-gdpr:BindingCorporateRules a rdfs:Class ;
    rdfs:label "Binding Corporate Rules (BCR)"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Binding corporate rules (BCR) are data protection policies adhered to by companies established in the EU for transfers of personal data outside the EU within a group of undertakings or enterprises."@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_20/oj> ;
    rdfs:subClassOf dpv-gdpr:DataTransferTool ;
    sw:term_status "accepted"@en .

dpv-gdpr:CertificationMechanismsForDataTransfers a rdfs:Class ;
    rdfs:label "Certification Mechanisms for Data Transfers"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Certification and its binding or specified mechanisms intended to provide sufficient safeguards for data transfers"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf> ;
    rdfs:subClassOf dpv-gdpr:DataTransferTool ;
    sw:term_status "accepted"@en .

dpv-gdpr:CodesOfConductForDataTransfers a rdfs:Class ;
    rdfs:label "Codes of Conduct for Data Transfers"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Codes of Conduct that outline sufficient safeguards for carrying out data transfers"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf> ;
    rdfs:subClassOf dpv-gdpr:DataTransferTool ;
    sw:term_status "accepted"@en .

dpv-gdpr:DataTransferTool a rdfs:Class ;
    rdfs:label "Data Transfer Tool"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A legal instrument or tool intended to assist or justify data transfers"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/sites/default/files/consultation/edpb_recommendations_202001_supplementarymeasurestransferstools_en.pdf>,
        <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/pnt_c/oj> ;
    rdfs:subClassOf dpv:TechnicalOrganisationalMeasure ;
    sw:term_status "accepted"@en .

dpv-gdpr:SCCByCommission a rdfs:Class ;
    rdfs:label "SCCs adopted by Commission"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Standard contractual clauses adopted by the Commission in accordance with the examination procedure referred to in GDPR Article 93(2)"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_c/oj> ;
    rdfs:subClassOf dpv-gdpr:StandardContractualClauses ;
    sw:term_status "accepted"@en .

dpv-gdpr:SCCBySupervisoryAuthority a rdfs:Class ;
    rdfs:label "SCCs adopted by Supervisory Authority"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Standard data protection clauses adopted by a supervisory authority and approved by the Commission pursuant to the examination procedure referred to in GDPR Article 93(2)"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_46/par_2/pnt_d/oj> ;
    rdfs:subClassOf dpv-gdpr:StandardContractualClauses ;
    sw:term_status "accepted"@en .

dpv-gdpr:StandardContractualClauses a rdfs:Class ;
    rdfs:label "Standard Contractual Clauses (SCC)"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Standard Contractual Clauses (SCCs) are pre-approved clauses by the EU for ensuring appropriate data protection safeguards intended for data transfers from the EU to third countries"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/dec_impl/2021/914/oj> ;
    rdfs:subClassOf dpv:Contract,
        dpv-gdpr:DataTransferTool ;
    sw:term_status "accepted"@en .

dpv-gdpr:SupplementaryMeasure a rdfs:Class ;
    rdfs:label "Supplementary Measure"@en ;
    dct:created "2021-09-22"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Supplementary measures are intended to additionally provide safeguards or guarentees to bring the resulting protection in line with EU requirements"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/system/files/2021-06/edpb_recommendations_202001vo.2.0_supplementarymeasurestransferstools_en.pdf> ;
    rdfs:subClassOf dpv:TechnicalOrganisationalMeasure ;
    sw:term_status "accepted"@en .

