    sw:term_status "accepted"@en .

dpv:Erase a rdfs:Class ;
    rdfs:label "Erase"@en ;
    dct:created "2019-05-07"^^xsd:date ;
    dct:description "to delete data"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_2/oj> ;
