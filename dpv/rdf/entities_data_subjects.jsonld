  {
    "@id": "http://www.w3.org/ns/dpv#Child",
    "@type": [
      "http://www.w3.org/2000/01/rdf-schema#Class"
    ],
    "http://purl.org/dc/terms/created": [
      {
        "@type": "http://www.w3.org/2001/XMLSchema#date",
        "@value": "2020-11-25"
      }
    ],
    "http://purl.org/dc/terms/creator": [
      {
        "@value": "Harshvardhan J. Pandit"
      }
    ],
    "http://purl.org/dc/terms/description": [
      {
        "@language": "en",
        "@value": "A 'child' is a natural legal person who is below a certain legal age depending on the legal jurisdiction."
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#comment": [
      {
        "@language": "en",
        "@value": "The legality of age defining a child varies by jurisdiction. In addition, 'child' is distinct from a 'minor'. For example, the legal age drinking alcohol can be 21, which makes a person of age 20 a 'minor' in this context. In other cases, 'minor' and 'child' are used interchangeably to refer to a person below some legally defined age."
      }
    ],
    "http://www.w3.org/2000/01/rdf-schema#label": [
      {
        "@language": "en",
        "@value": "Child"
      }
    ],
