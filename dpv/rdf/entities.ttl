@prefix dct: <http://purl.org/dc/terms/> .
@prefix dpv: <http://www.w3.org/ns/dpv#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sw: <http://www.w3.org/2003/06/sw-vocab-status/ns#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dpv:AcademicScientificOrganisation a rdfs:Class ;
    rdfs:label "AcademicScientificOrganisation"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Universities, Schools, Research Bodies"@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:Authority a rdfs:Class ;
    rdfs:label "Authority"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg Krog"^^xsd:string,
        "Harshvardhan Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "An authority with the power to create or enforce laws, or determine their compliance."@en ;
    rdfs:subClassOf dpv:LegalEntity ;
    sw:term_status "accepted"@en .

dpv:DataExporter a rdfs:Class ;
    rdfs:label "Data Exporter"@en ;
    dct:created "2021-09-08"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg Krog"^^xsd:string,
        "Harshvardhan Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "An entity that 'exports' data where exporting is considered a form of data transfer"@en ;
    rdfs:comment "The EU, in particular the EDPB, uses data exporter the context of cross-border data transfers/flows. These concepts are not bound by jurisdictional or geopolitical scopes within DPV and can thus be used for any notion of exporting"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/our-work-tools/our-documents/recommendations/recommendations-012020-measures-supplement-transfer_en> ;
    rdfs:subClassOf dpv:LegalEntity ;
    sw:term_status "accepted"@en .

dpv:DataImporter a rdfs:Class ;
    rdfs:label "Data Importer"@en ;
    dct:created "2021-09-08"^^xsd:date ;
    dct:creator "David Hickey"^^xsd:string,
        "Georg Krog"^^xsd:string,
        "Harshvardhan Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "An entity that 'imports' data where importing is considered a form of data transfer"@en ;
    rdfs:comment "The EU, in particular the EDPB, uses data importing the context of cross-border data transfers/flows. These concepts are not bound by jurisdictional or geopolitical scopes within DPV and can thus be used for any notion of importing"@en ;
    rdfs:isDefinedBy <https://edpb.europa.eu/our-work-tools/our-documents/recommendations/recommendations-012020-measures-supplement-transfer_en> ;
    rdfs:subClassOf dpv:Recipient ;
    sw:term_status "accepted"@en .

dpv:DataProcessor a rdfs:Class ;
    rdfs:label "Data Processor"@en ;
    dct:created "2019-06-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A ‘processor’ means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller."@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_8/oj> ;
    rdfs:subClassOf dpv:Recipient ;
    sw:term_status "accepted"@en .

dpv:DataProtectionAuthority a rdfs:Class ;
    rdfs:label "Data Protection Authority"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Georg Krog"^^xsd:string,
        "Harshvardhan Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "An authority tasked with overseeing legal compliance regarding privacy and data protection laws."@en ;
    rdfs:subClassOf dpv:Authority ;
    sw:term_status "accepted"@en .

dpv:DataSubProcessor a rdfs:Class ;
    rdfs:label "Data Sub-Processor"@en ;
    dct:created "2020-11-25"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A 'sub-processor' is a processor engaged by another processor"@en ;
    rdfs:comment "sub-processor' is a commonly used term similar to 'sub-contractor' and does not have a specific legal definition"@en ;
    rdfs:subClassOf dpv:DataProcessor ;
    sw:term_status "accepted"@en .

dpv:ForProfitOrganisation a rdfs:Class ;
    rdfs:label "ForProfitOrganisation"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description ""@en ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:GovernmentalOrganisation a rdfs:Class ;
    rdfs:label "GovernmentalOrganisation"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "e.g. tax department ; should DPA go under these?"@en ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:IndustryConsortium a rdfs:Class ;
    rdfs:label "IndustryConsortium"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description ""@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:JointDataControllers a rdfs:Class ;
    rdfs:label "Joint Data Controllers"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Georg Krog"^^xsd:string,
        "Harshvardhan Pandit"^^xsd:string ;
    dct:description "A group of Data Controllers that jointly determine the purposes and means of processing"@en ;
    rdfs:comment "To indicate the membership, hasDataController may be used"@en ;
    rdfs:subClassOf dpv:DataController ;
    sw:term_status "accepted"@en .

dpv:LegalEntity a rdfs:Class ;
    rdfs:label "Legal Entity"@en ;
    dct:created "2019-04-05"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A human or non-human that constitute as a legally defined entity"@en ;
    sw:term_status "accepted"@en .

dpv:LegalRole a rdfs:Class ;
    rdfs:label "Legal Role"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Represents the 'role' played by an entity"@en ;
    rdfs:comment "concepts are both legal entity and legal roles"@en ;
    sw:term_status "accepted"@en .

dpv:NationalAuthority a rdfs:Class ;
    rdfs:label "NationalAuthority"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "e.g. DPA, but not all \"DPA\" are national authority e.g. EDPS and EDPB"@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Authority,
        dpv:GovernmentalOrganisation ;
    sw:term_status "accepted"@en .

dpv:NonGovernmentalOrganisation a rdfs:Class ;
    rdfs:label "NonGovernmentalOrganisation"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description ""@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:NonProfitOrganisation a rdfs:Class ;
    rdfs:label "NonProfitOrganisation"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description ""@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Organisation ;
    sw:term_status "accepted"@en .

dpv:Organisation a rdfs:Class ;
    rdfs:label ""@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description ""@en ;
    rdfs:subClassOf dpv:LegalEntity ;
    sw:term_status "accepted"@en .

dpv:RegionalAuthority a rdfs:Class ;
    rdfs:label "RegionalAuthority"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "e.g. regional DPA in DE / US-CA"@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Authority,
        dpv:GovernmentalOrganisation ;
    sw:term_status "accepted"@en .

dpv:Representative a rdfs:Class ;
    rdfs:label "Representative"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg Krog"^^xsd:string,
        "Harshvardhan J. Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "A representative of a legal entity"@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_27/oj> ;
    rdfs:subClassOf dpv:LegalEntity ;
    sw:term_status "accepted"@en .

dpv:SupraNationalAuthority a rdfs:Class ;
    rdfs:label "SupraNationalAuthority"@en ;
    dct:created "2022-02-02"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "e.g. EU, EDPB"@en ;
    rdfs:isDefinedBy <http://purl.org/adms> ;
    rdfs:subClassOf dpv:Authority,
        dpv:GovernmentalOrganisation ;
    sw:term_status "accepted"@en .

dpv:ThirdParty a rdfs:Class ;
    rdfs:label "Third Party"@en ;
    dct:created "2019-06-04"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "A ‘third party’ means a natural or legal person, public authority, agency or body other than the data subject, controller, processor and people who, under the direct authority of the controller or processor, are authorised to process personal data."@en ;
    rdfs:isDefinedBy <https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_10/oj> ;
    rdfs:subClassOf dpv:Recipient ;
    sw:term_status "accepted"@en .

dpv:hasAddress a rdf:Property ;
    rdfs:label "has address"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies address of a legal entity such as street address or pin code"@en ;
    sw:term_status "accepted"@en .

dpv:hasContact a rdf:Property ;
    rdfs:label "has contact"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies contact details of a legal entity such as phone  or email"@en ;
    sw:term_status "accepted"@en .

dpv:hasIdentifier a rdf:Property ;
    rdfs:label "has identifier"@en ;
    dct:created "2020-11-25"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies an identifier for the entity such as registeration number or official ID"@en ;
    sw:term_status "accepted"@en .

dpv:hasName a rdf:Property ;
    rdfs:label "has name"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies name of a legal entity"@en ;
    sw:term_status "accepted"@en .

dpv:hasRepresentative a rdf:Property ;
    rdfs:label "has representative"@en ;
    dct:created "2020-11-04"^^xsd:date ;
    dct:creator "Beatriz Esteves"^^xsd:string,
        "Georg P Krog"^^xsd:string,
        "Harshvardhan J.Pandit"^^xsd:string,
        "Paul Ryan"^^xsd:string ;
    dct:description "Specifies representative of the legal entity"@en ;
    sw:term_status "accepted"@en .

