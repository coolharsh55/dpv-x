  <rdf:Description rdf:about="https://w3id.org/dpv#Pseudonymisation">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#DeterministicPseudonymisation"/>
    <skos:narrower rdf:resource="https://w3id.org/dpv#RNGPseudonymisation"/>
