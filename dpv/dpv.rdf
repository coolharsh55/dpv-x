  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#PseudoAnonymization">
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Axel Polleres</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Rob Brennan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Mark Lizar</dct:creator>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <dct:description xml:lang="en">PseudoAnonmyization or 'pseudonymisation’ means the processing of personal data in such a manner that the personal data can no longer be attributed to a specific data subject without the use of additional information, provided that such additional information is kept separately and is subject to technical and organisational measures to ensure that the personal data are not attributed to an identified or identifiable natural person;</dct:description>
    <rdfs:label xml:lang="en">Pseudo-Anonymization</rdfs:label>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#PseudonymisationEncryption"/>
    <rdfs:isDefinedBy rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_5/oj"/>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
