    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:label xml:lang="en">GuidelinesPrinciple</rdfs:label>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#Sector">
    <rdfs:label xml:lang="en">Sector</rdfs:label>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fajar Ekaputra</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Javier Fernandez</dct:creator>
