    <dct:contributor>Eva Schlehahn</dct:contributor>
    <dct:contributor>Ramisa Gachpaz Hamed</dct:contributor>
    <dct:abstract xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about legally compliant personal data handling, which includes purposes, processing, personal data, technical and organisational measures.</dct:abstract>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Ontology"/>
    <dct:source rdf:resource="https://www.w3.org/community/dpvcg/"/>
    <vann:preferredNamespacePrefix>dpv</vann:preferredNamespacePrefix>
    <dct:description xml:lang="en">The Data Privacy Vocabulary (DPV) provides terms (classes and properties) to represent information about legally compliant personal data handling, which includes purposes, processing, personal data, technical and organisational measures.</dct:description>
    <vann:preferredNamespaceUri rdf:datatype="http://www.w3.org/2001/XMLSchema#string">https://w3.org/ns/dpv</vann:preferredNamespaceUri>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-01</dct:modified>
    <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#string">0.4</owl:versionInfo>
  </rdf:Description>
