    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <skos:note xml:lang="en">This purpose relates specifiaclly for record creation and management. This can be combined or used along with other purposes to express intentions such as records for legal compliance or vendor payments.</skos:note>
    <skos:definition xml:lang="en">Purposes associated with manage creation, storage, and use of records relevant to operations, events, and processes e.g. to store logs or access requests</skos:definition>
    <skos:prefLabel xml:lang="en">Record Management</skos:prefLabel>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:broader rdf:resource="https://w3id.org/dpv#Purpose"/>
  </rdf:Description>
