    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:definition xml:lang="en">Personal Data that has been (fully and completely) anonymised so that it is no longer considered Personal Data</skos:definition>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#NonPersonalData"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
