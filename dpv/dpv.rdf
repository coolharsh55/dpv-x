  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#DerivedPersonalData">
    <rdfs:label xml:lang="en">Derived Personal Data</rdfs:label>
    <rdf:type rdf:resource="http://www.w3.org/2000/01/rdf-schema#Class"/>
    <rdfs:subClassOf rdf:resource="http://www.w3.org/ns/dpv#PersonalData"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-05-07</dct:created>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Elmar Kiesling; Harshvardhan J. Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Fajar Ekaputra</dct:creator>
    <rdfs:seeAlso rdf:resource="http://www.specialprivacy.eu/vocabs/data#Derived"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:description xml:lang="en">Personal Data that is obtained or derived from other data</dct:description>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-01-19</dct:modified>
    <rdfs:comment xml:lang="en">Derived Data is data that is obtained through processing of existing data, e.g. deriving first name from full name. To indicate data that is derived but which was not present or evident within the source data, InferredPersonalData should be used.</rdfs:comment>
    <rdfs:isDefinedBy rdf:resource="https://www.w3.org/community/dpvcg/"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://www.w3.org/ns/dpv#SingleSignOn">
