  <rdf:Description rdf:about="https://w3id.org/dpv#PassiveRight">
    <skos:inScheme rdf:resource="https://w3id.org/dpv#"/>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv#"/>
    <skos:prefLabel xml:lang="en">Passive Right</skos:prefLabel>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <rdf:type rdf:resource="https://w3id.org/dpv#Concept"/>
    <skos:definition xml:lang="en">The right(s) applicable, provided, or expected that are always (passively) applicable</skos:definition>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J Pandit</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Beatriz Esteves</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg P Krog</dct:creator>
    <skos:broader rdf:resource="https://w3id.org/dpv#Right"/>
    <skos:note xml:lang="en">Passive rights do not require the entity to request or exercise them. They are considered to be always applicable. For example, the Right to Privacy (in EU) does not require an exercise for it to be fulfilled.</skos:note>
    <dpv:isSubTypeOf rdf:resource="https://w3id.org/dpv#Right"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-10-22</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#ConsentRequestDeferred">
