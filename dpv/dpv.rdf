    <rdfs:label xml:lang="en">Legal Basis</rdfs:label>
    <dct:description xml:lang="en">The Legal basis used to justify processing of personal data</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
