    rdfs:isDefinedBy dpv: ;
    rdfs:range dpv:Recipient ;
    rdfs:subPropertyOf dpv:hasEntity ;
    sw:term_status "accepted"@en ;
    skos:definition "Indicates Recipient of Personal Data"@en ;
