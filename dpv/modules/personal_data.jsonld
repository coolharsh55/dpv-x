      {
        "@id": "https://w3id.org/dpv#"
      }
    ],
    "http://www.w3.org/2003/06/sw-vocab-status/ns#term_status": [
      {
        "@language": "en",
        "@value": "accepted"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#definition": [
      {
        "@language": "en",
        "@value": "Personal Data that has undergone a partial (incomplete) anonymisation process such that it is still considered Personal Data"
      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "Pseudo-anonymised Data"
      }
    ],
