      }
    ],
    "http://www.w3.org/2004/02/skos/core#prefLabel": [
      {
        "@language": "en",
        "@value": "isSubTypeOf"
      }
    ]
  },
  {
    "@id": "https://w3id.org/dpv#PseudoAnonymisedData",
