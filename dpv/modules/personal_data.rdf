    <skos:prefLabel xml:lang="en">isSubTypeOf</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasRange">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <skos:prefLabel xml:lang="en">has range</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#hasDomain">
    <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2004/02/skos/core#semanticRelation"/>
    <skos:prefLabel xml:lang="en">has domain</skos:prefLabel>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv#Concept">
    <rdfs:subClassOf rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">Concept</skos:prefLabel>
  </rdf:Description>
</rdf:RDF>
