    <skos:member rdf:resource="https://w3id.org/dpv#InferredPersonalData"/>
    <skos:member rdf:resource="https://w3id.org/dpv#PseudoAnonymisedData"/>
    <skos:member rdf:resource="https://w3id.org/dpv#SensitivePersonalData"/>
    <skos:member rdf:resource="https://w3id.org/dpv#SpecialCategoryPersonalData"/>
    <skos:member rdf:resource="https://w3id.org/dpv#Data"/>
