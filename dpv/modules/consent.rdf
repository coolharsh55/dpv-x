  <rdf:Description rdf:about="https://w3id.org/dpv#hasWithdrawalMethod">
    <skos:editorialNote xml:lang="en">WARNING: This concept will be deprecated in future releases</skos:editorialNote>
    <rdf:type rdf:resource="https://w3id.org/dpv#Relation"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:prefLabel xml:lang="en">has withdrawal method</skos:prefLabel>
    <skos:definition xml:lang="en">Specifries the method by which consent can be/has been withdrawn</skos:definition>
    <skos:note xml:lang="en">Can be used to record information of how to withdraw consent, e.g. by a click to a form, in writing, etc., by logging into a system and confirm per email, or with some additional authentication, etc.</skos:note>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-04-05</dct:created>
