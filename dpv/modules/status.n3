dpv:hasActivityStatus a skos:Concept,
        dpv:Relation ;
    dct:created "2022-05-18"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    rdfs:isDefinedBy dpv: ;
    sw:term_status "accepted"@en ;
    skos:broader dpv:hasStatus ;
