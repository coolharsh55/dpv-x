    dct:created "2022-05-18"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:modified "2022-09-07"^^xsd:date ;
    rdfs:isDefinedBy dpv: ;
    sw:term_status "changed"@en ;
    skos:broader dpv:ComplianceStatus ;
