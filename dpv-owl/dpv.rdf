    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-01</dct:created>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Purpose"/>
    <rdfs:label xml:lang="en">Record Management</rdfs:label>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
