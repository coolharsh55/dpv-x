  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Pseudonymisation">
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#DataAnonymisationTechnique"/>
