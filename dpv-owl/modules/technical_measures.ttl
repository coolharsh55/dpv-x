dpvo:CompleteAnonymisation a owl:Class ;
    rdfs:label "Complete Anonymisation"@en ;
    dct:created "2022-02-09"^^xsd:date ;
    dct:creator "Harshvardhan J. Pandit"^^xsd:string ;
    dct:description "Altering personal data irreversibly such that a data subject can no longer be identified directly or indirectly, either by the data controller alone or in collaboration with any other party"@en ;
    rdfs:isDefinedBy dpvo: ;
    rdfs:subClassOf dpvo:Anonymization ;
    sw:term_status "accepted"@en .

