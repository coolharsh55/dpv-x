    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#DataProtectionOfficer">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Representative"/>
    <rdfs:label xml:lang="en">Data Protection Officer</rdfs:label>
    <dct:description xml:lang="en">An entity within or authorised by an organisation to monitor internal compliance, inform and advise on data protection obligations and act as a contact point for data subjects and the supervisory authority.</dct:description>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_37/oj"/>
