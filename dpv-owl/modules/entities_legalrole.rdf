    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#DataProcessor">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Recipient"/>
    <rdfs:label xml:lang="en">Data Processor</rdfs:label>
    <dct:description xml:lang="en">A ‚Äòprocessor‚Äô means a natural or legal person, public authority, agency or other body which processes personal data on behalf of the controller.</dct:description>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_4/par_8/oj"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2019-06-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
