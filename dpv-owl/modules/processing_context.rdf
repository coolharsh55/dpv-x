  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#AlgorithmicLogic">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#AutomationOfProcessing"/>
