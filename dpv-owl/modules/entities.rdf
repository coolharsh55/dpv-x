>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#hasRecipientDataController">
    <rdf:type rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#Property"/>
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#ObjectProperty"/>
    <rdfs:domain rdf:resource="http://www.w3.org/2002/07/owl#Thing"/>
    <rdfs:range rdf:resource="https://w3id.org/dpv/dpv-owl#DataController"/>
    <rdfs:subPropertyOf rdf:resource="https://w3id.org/dpv/dpv-owl#hasRecipient"/>
    <rdfs:label xml:lang="en">has recipient data controller</rdfs:label>
    <dct:description xml:lang="en">Indiciates inclusion or applicability of a Data Controller as a Recipient of persona data</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-11</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
