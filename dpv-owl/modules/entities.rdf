  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#NonGovernmentalOrganisation">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#Organisation"/>
    <rdfs:label xml:lang="en">NonGovernmentalOrganisation</rdfs:label>
    <dct:description xml:lang="en">An organisation not part of or independent from the government</dct:description>
    <dct:source rdf:resource="http://purl.org/adms"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-02-02</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Harshvardhan J. Pandit</dct:creator>
    <rdfs:isDefinedBy rdf:resource="https://w3id.org/dpv/dpv-owl#"/>
  </rdf:Description>
  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl#Authority">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#Class"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LegalEntity"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#GovernmentalOrganisation"/>
    <rdfs:label xml:lang="en">Authority</rdfs:label>
    <dct:description xml:lang="en">An authority with the power to create or enforce laws, or determine their compliance.</dct:description>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2020-11-04</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Georg Krog</dct:creator>
    <dct:creator rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Paul Ryan</dct:creator>
