  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/risk#SWIFT">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl/risk#QualitativeRiskAssessmentTechnique"/>
    <rdfs:label xml:lang="en">Structured "What If?" (SWIFT)</rdfs:label>
    <dct:description xml:lang="en">A simpler form of HAZOP with prompts of "what if" to identify deviations from the expected.</dct:description>
    <dct:source rdf:resource="https://www.iso.org/standard/72140.html"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
