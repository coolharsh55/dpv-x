  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/risk#CauseConsequenceAnalysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl/risk#QuantitativeRiskAssessmentTechnique"/>
    <rdfs:label xml:lang="en">Cause-Consequence Analysis</rdfs:label>
    <dct:description xml:lang="en">A combination of fault and event tree analysis that allows inclusion of time delays. Both causes and consequences of an initiating event are considered.</dct:description>
    <dct:source rdf:resource="https://www.iso.org/standard/72140.html"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
    <sw:term_status xml:lang="en">accepted</sw:term_status>
