  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/risk#CrossImpactAnalysis">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl/risk#QuantitativeRiskAssessmentTechnique"/>
    <rdfs:label xml:lang="en">Cross Impact Analysis</rdfs:label>
    <dct:description xml:lang="en">Evaluates changes in the probability of the occurrence of a given set of events consequent on the actual occurrence of one of them.</dct:description>
    <dct:source rdf:resource="https://www.iso.org/standard/72140.html"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
