  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/risk#CVaR">
    <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#NamedIndividual"/>
    <rdf:type rdf:resource="https://w3id.org/dpv/dpv-owl/risk#QuantitativeRiskAssessmentTechnique"/>
    <rdfs:label xml:lang="en">Conditional Value at Risk (CVaR)</rdfs:label>
    <dct:description xml:lang="en">A measure of the expected loss from a financial portfolio in the worst a % of cases. Also called expected shortfall (ES)</dct:description>
    <dct:source rdf:resource="https://www.iso.org/standard/72140.html"/>
    <dct:created rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2022-08-18</dct:created>
