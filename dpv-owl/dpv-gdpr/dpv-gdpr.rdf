  <rdf:Description rdf:about="https://w3id.org/dpv/dpv-owl/dpv-gdpr#A49-2">
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#LegitimateInterest"/>
    <rdfs:subClassOf rdf:resource="https://w3id.org/dpv/dpv-owl#DataTransferLegalBasis"/>
    <dct:modified rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2021-09-08</dct:modified>
    <dct:source rdf:resource="https://eur-lex.europa.eu/eli/reg/2016/679/art_49/par_2/oj"/>
